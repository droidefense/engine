  var stringChartCanvas = $("#stringChart").get(0).getContext("2d");
  var stringChart = new Chart(stringChartCanvas);
  var PieData = [
    {
      value: 700,
      color: "#f56954",
      highlight: "#f56954",
      label: "Chrome"
    },
    {
      value: 500,
